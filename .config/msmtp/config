account simon@simonxiang.xyz
host mail.simonxiang.xyz
port 587
from simon@simonxiang.xyz
user simon@simonxiang.xyz
passwordeval "pass simon@simonxiang.xyz"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/simon/.config/msmtp/msmtp.log

